<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

  <grid name="title-grid" title="Title"
    model="com.axelor.contact.db.Title">
    <field name="code" />
    <field name="name" />
  </grid>
  <form name="title-form" title="Title"
    model="com.axelor.contact.db.Title" width="mid">
    <panel title="Title Details">
      <field name="code" colSpan="5" />
      <field name="name" colSpan="5" />

    </panel>

    <panel-tabs>
      <panel title="Notes" itemSpan="6">
        <field name="createdOn"></field>
        <field name="createdOn"></field>
      </panel>

      <panel title="Link">
        <field name="createdOn"></field>
        <field name="createdOn"></field>
      </panel>
    </panel-tabs>


    <panel-stack showIf="color">
      <panel title="Page 1" />
      <panel title="Page 2" />
      <panel title="Page 3" />
    </panel-stack>

    <panel>
      <field name="fullName">
       	<viewer >VISHAL </viewer>
      </field>
    </panel>

  </form>
</object-views>
